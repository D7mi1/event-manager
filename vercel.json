{
  "buildCommand": "npm run build",
  "framework": "nextjs",
  "nodeVersion": "20.x",
  "installCommand": "npm install",
  "env": {
    "NEXT_PUBLIC_SUPABASE_URL": "@supabase_url",
    "NEXT_PUBLIC_SUPABASE_ANON_KEY": "@supabase_key",
    "NEXT_PUBLIC_SENTRY_DSN": "@sentry_dsn"
  },
  "envPrefix": "NEXT_PUBLIC_",
  "regions": ["iad"],
  "functions": {
    "api/**/*.ts": {
      "memory": 1024,
      "maxDuration": 60
    }
  }
}
